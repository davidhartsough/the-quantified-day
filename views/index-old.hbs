                        ['Reading', reading],
                        ['In Meetings', meetings],
                        ['Working', working],
                        ['In Classes', inclass],
                        ['Watching TV', tv],
                        ['Household Chores', household],
                        ['Exercising', exercising],
                        ['Hobby-ing', hobby],
                        ['EXTRA TIME!', extraTime]
                ]);
                var options = {};
                var chart = new google.visualization.PieChart(document.getElementById('free-sample'));
                chart.draw(data, options);
            };
