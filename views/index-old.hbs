    <script type="text/javascript">
        $(function() {
            $(document).foundation();
            $('.second-shown').hide();
            $('.errors').hide();
            $('.oops').hide();



            $('.js-sign-in').click(function() {
                var login = $("#login").val();
                if (login != "") {
                    $.get('/api/v1/average-day/' + login, function(resp) {
                        if (resp != null) {
                            $('.first-shown').hide();
                            $('.second-shown').show();
                            
                            $('.js-header').text(resp.username + "\'s Average Day");
                            
                            
                            var newData = google.visualization.arrayToDataTable([
                                ['Activity', 'Minutes per Day'],
                                ['Sleeping', resp.sleeping],
                                ['Showering', resp.showering],
                                ['Grooming', resp.grooming],
                                ['Restroom-ing', resp.peeing],
                                ['Cooking', resp.cooking],
                                ['Eating', resp.eating],
                                ['Driving', resp.driving],
                                ['Walking', resp.walking],
                                ['Biking', resp.biking],
                                ['Riding', resp.riding],
                                ['Chatting', resp.chatting],
                                ['Emailing', resp.emailing],
                                ['Texting', resp.texting],
                                ['Calling', resp.calling],
                                ['Gaming', resp.gaming],
                                ['Social Media', resp.socialmedia],
                                ['Web Browsing', resp.browsing],
                                ['Reading', resp.reading],
                                ['In Meetings', resp.meetings],
                                ['Working', resp.working],
                                ['In Classes', resp.inclass],
                                ['Watching TV', resp.tv],
                                ['Household Chores', resp.household],
                                ['Exercising', resp.exercising],
                                ['Hobby-ing', resp.hobby],
                                ['EXTRA TIME!', resp.extraTime]
                            ]);
                            var newOptions = {};
                            var newChart = new google.visualization.PieChart(document.getElementById('piechart'));
                            newChart.draw(newData, newOptions);
                        } else {
                            $('.oops').show();
                        }
                    });
                } else {
                    $('.oops').show();
                }
            });

            $('.js-submit-day').click(function() {
